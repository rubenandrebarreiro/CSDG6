		<relativePath/> <!-- lookup parent from repository -->
	</parent>
	<groupId>fct.unl.pt</groupId>
	<artifactId>csdw1</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>csdw1</name>
	<description>Work A. 1</description>

	<properties>
		<java.version>1.8</java.version>
	</properties>

	<dependencies>
